<Window x:Class="SoundPad_WPF_8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SoundPad_WPF_8"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Sounds_Btn" Content="Sounds" HorizontalAlignment="Left" Height="60" Margin="730,10,0,0" VerticalAlignment="Top" Width="60"/>
        <Button x:Name="New_Sound_Btn" Content="New Sound" HorizontalAlignment="Left" Height="60" Margin="600,10,0,0" VerticalAlignment="Top" Width="110" Click="New_Sound_Btn_Click"/>
        <Button x:Name="Save_Btn" Content="Save" HorizontalAlignment="Left" Height="60" Margin="520,10,0,0" VerticalAlignment="Top" Width="60" Click="Save_Btn_Click"/>
        <Button x:Name="Options_Btn" Content="Options" HorizontalAlignment="Left" Height="60" Margin="10,10,0,0" VerticalAlignment="Top" Width="60"/>
        <StackPanel x:Name="SoundPack" HorizontalAlignment="Left" Height="345" Margin="400,79,0,0" VerticalAlignment="Top" Width="390"/>

    </Grid>
</Window>
